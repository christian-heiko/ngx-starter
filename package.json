{
  "name": "@elderbyte/ngx-starter",
  "version": "0.0.3",
  "description": "Common modules library for creating web apps with Angular / Material",
  "main": "dist/bundles/ngx-starter.umd.js",
  "module": "dist/index.js",
  "scripts": {
    "cleanup": "rimraf dist",
    "copy": "copyfiles src/**/*.scss dist && copyfiles src/**/*.html dist && copyfiles LICENSE README.md dist",
    "bundling": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-starter.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-starter.umd.min.js",
    "build": "npm run cleanup && ngc && npm run copy && npm run bundling && npm run minify"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular 2",
    "starter",
    "bootstrap",
    "common",
    "angular",
    "material"
  ],
  "author": "Elderbyte",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElderByte-/ngx-starter.git"
  },
  "homepage": "https://github.com/ElderByte-/ngx-starter",
  "bugs": {
    "url": "https://github.com/ElderByte-/ngx-starter/issues"
  },
  "dependencies": {
    "@angular/animations": "^4.3.3",
    "@angular/common": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/http": "^4.3.3",
    "@angular/router": "^4.3.3",
    "@angular/forms": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-server": "^4.3.3",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/material": "^2.0.0-beta.8",
    "@ngx-translate/core": "^7.1.0",
    "@elderbyte/ngx-jwt-auth": "~0.1.4",
    "@elderbyte/ngx-simple-webstorage": "~0.1.1",
    "reflect-metadata": "~0.1.10",
    "rxjs": "^5.2.0",
    "zone.js": "~0.8.5",
    "angular2-jwt": "^0.2.2"
  },
  "devDependencies": {
    "@angular/cli": "1.2.6",
    "@angular/compiler": "4.3.3",
    "@angular/compiler-cli": "^4.3.3",
    "clean-css": "^3.4.23",
    "copyfiles": "^1.2.0",
    "html-minifier": "^3.3.1",
    "node-sass": "^4.1.1",
    "rimraf": "^2.6.1",
    "rollup": "~0.45.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-angular-aot": "^0.0.4",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "^5.0.3",
    "typescript": "^2.4.2",
    "uglify-js": "^2.8.20"
  }
}
