{
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-starter -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "update:ng": "ng update"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest.ts",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ElderByte-/ngx-starter"
  },
  "author": {
    "name": "ElderByte AG",
    "email": "info@elderbyte.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ElderByte-/ngx-starter/issues"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.2.1",
    "@angular/core": "^6.0.5",
    "@angular/common": "^6.0.5",
    "@angular/compiler": "^6.0.5",
    "@angular/compiler-cli": "^6.0.5",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.5",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/router": "^6.0.5",
    "@elderbyte/ngx-simple-webstorage": "^6.0.0",
    "@elderbyte/ts-logger": "^0.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "tslint": "^5.10.0",
    "codelyzer": "^4.3.0",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "jest": "^22.4.3",
    "jest-preset-angular": "^5.2.2",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "rollup": "^0.41.6",
    "run-sequence": "^1.2.2",
    "rxjs": "^6.2.1",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.1",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
